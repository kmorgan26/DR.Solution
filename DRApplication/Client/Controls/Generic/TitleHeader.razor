<MudText Typo="Typo.h3" Class="mb-6 pl-0">
    <MudPaper Class="pa-4" Elevation="0">
        <MudIcon Icon="@IconPath" Size="Size.Large" Class="m-3" />
        @TitleText
    </MudPaper>
</MudText>

@code {

    [Parameter]
    public string TitleText { get; set; }

    [Parameter]
    public string IconPath { get; set; }

}
