@namespace DRApplication.Client.Pages
@page "/currentload/edit/{CurrentLoadId:int}"

<TitleHeader TitleText="Current Load Edit" IconPath="@Icons.Material.Filled.Checklist" />

<MudPaper Class="pa-2 mb-6" Elevation="0">
    <MudAlert Severity="Severity.Success">Select a current Load for this Device, then click Update</MudAlert>
</MudPaper>

<CurrentLoadEditComponent CurrentLoadId="@CurrentLoadId" />

@code{

    [Parameter]
    public int CurrentLoadId { get; set; }

}